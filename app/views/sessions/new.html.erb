<h1> login </h1>




<%= form_tag do %>



<table>
	<tr>
		<td><%= label_tag(:user_name) %></td>
		<td><%= text_field_tag(:user_name) %></td>
	</tr>
	<tr>
		<td><%= label_tag(:password) %></td>
		<td><%= password_field_tag(:password) %></td>
	<tr>
		<td><%= submit_tag "sign_in" %></td>
	</tr>
	
</table>
<% end %>